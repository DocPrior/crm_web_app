<h1>All Contacts</h1>
<ul>
  <% @contacts.each do |contact| %>
      <li> <%= contact.first_name %>
        <%=contact.last_name %>
        Email: <%= contact.email %>
        Note: <%= contact.note %>
        <a href="/contacts/<%= contact.id %>"> ID: <%= contact.id %></a>  </li>
    <%end%>
</ul>
